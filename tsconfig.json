{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "./",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",

    "paths": {
      "@app/models": ["src/app/shared/models/index"],
      "@app/services": ["src/app/core/services/index"],
      "@app/constants": ["src/app/core/constants/index"],
      "@app/interceptors": ["src/app/core/interceptors/index"],
      "@app/shared/*": ["src/app/shared/*"],
    }
  },
  "include": [
    "src"
  ]
}
